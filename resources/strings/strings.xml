<strings>
  <string id="AppName">GRun中文版</string>
  <string id="PARAM_APP_VERSION">GRun版本</string>

  <string id="PARAM_TARGET_PACE">目标配速 (当系统使用公制，秒每千米；当系统使用英制，秒每英里)</string>
  <string id="PARAM_DESC_TARGET_PACE">例如: 330（5*60+30）代表五分三十秒(单位千米/英里根据手表设置确定）</string>
  <string id="PARAM_PACE_RANGE">配速上下浮动时间 (单位：秒)</string>
  <string id="PARAM_DESC_PACE_RANGE">当配速不在区间内用颜色警示</string>

  <string id="PARAM_LAP_DISTANCE">圈距（单位：米）</string>
  <string id="PARAM_DESC_LAP_DISTANCE">当Lap键按下时自动修正距离 (0-禁用, 1000-1千米, 1609-1英里)</string>

  <string id="PARAM_HEADER_POSITION">标题头位置</string>
  <string id="PARAM_HEADER_POSITION_1">顶部/顶部</string>
  <string id="PARAM_HEADER_POSITION_2">顶部/底部</string>
  <string id="PARAM_HEADER_POSITION_3">底部/顶部</string>

  <string id="PARAM_HEADER_HEIGHT">头部高度百分比（0-100）</string>
  <string id="PARAM_DESC_HEADER_HEIGHT">0表示隐藏头部</string>

  <string id="PARAM_ROW_HEIGHT_RATIO">行高度比</string>
  <string id="PARAM_DESC_ROW_HEIGHT_RATIO">例如: 4,7,7,3,3 表示 第一行使用屏幕高度的4/24, 第二和第三行使用7/24, 依此类推</string>

  <string id="PARAM_COLUMN_WIDTH_RATIO_1">第二行的列宽比</string>
  <string id="PARAM_DESC_COLUMN_WIDTH_RATIO_1">例如: 1,2,1 意味着第二列将会是第一和第三列的两倍宽</string>
  <string id="PARAM_COLUMN_WIDTH_RATIO_2">第三行列宽比</string>

  <string id="PARAM_SINGLE_BACKGROUND_COLOR">统一背景颜色（如果启用，第四、五行将会和第一、二、三行颜色相同）</string>
  <string id="PARAM_DESC_SINGLE_BACKGROUND_COLOR">如果设置为true，第四、五行将会和第一、二、三行颜色相同</string>

  <string id="PARAM_DISABLE_COLOR">禁用</string>
  <string id="PARAM_ALL_BACKGROUND_COLOR">所有字段背景色</string>
