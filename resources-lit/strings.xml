    <string id="PARAM_TARGET_PACE">Target Pace (in seconds per km or mile)</string>
    <string id="PARAM_DESC_TARGET_PACE">Example: 330 for 5min and 30sec (km/mile is chosen
        automatically based on watch settings</string>
