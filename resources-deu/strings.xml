  <string id="AppName">GRun i18n</string>
  <string id="PARAM_APP_VERSION">GRun Version</string>

  <string id="PARAM_TARGET_PACE">Ziel-Geschw.(in Sek./km oder Meile)</string>
  <string id="PARAM_DESC_TARGET_PACE">Beispiel: 330 für 5min und 30sec (km/Meile wird
    automatisch basierend auf Uhr-Einstellungen gewählt)</string>
  <string id="PARAM_PACE_RANGE">Geschw.-Bereich (+/- in Sekunden)</string>
  <string id="PARAM_DESC_PACE_RANGE">Verwenden, um Geschw./Tempo in Farbe anzuzeigen,
    wenn außerhalb des Bereichs</string>

  <string id="PARAM_LAP_DISTANCE">Runden-Distanz (in Metern)</string>
  <string id="PARAM_DESC_LAP_DISTANCE">Automatische Korrektur der Distanz, wenn Runden-Taste
