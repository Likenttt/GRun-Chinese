  <string id="PARAM_HEADER_HEIGHT">Header-Höhe (%)</string>
  <string id="PARAM_DESC_HEADER_HEIGHT">0 zum Verbergen von Headern</string>

  <string id="PARAM_ROW_HEIGHT_RATIO">Zeilen-Höhe-Verhältnis</string>
  <string id="PARAM_DESC_ROW_HEIGHT_RATIO">Beispiel: 4,7,7,3,3 bedeutet, dass Zeile #1 4/24
    des Bildschirms verwendet, Zeile #2 und #3 7/24 usw.</string>

  <string id="PARAM_COLUMN_WIDTH_RATIO_1">Spaltenbreiten-Verhältnis für zweite Zeile</string>
  <string id="PARAM_DESC_COLUMN_WIDTH_RATIO_1">Beispiel: 1,2,1 bedeutet, dass die mittlere
