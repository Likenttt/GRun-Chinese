  <string id="AppName">GRun i18n</string>
  <string id="PARAM_APP_VERSION">Version GRun</string>

  <string id="PARAM_TARGET_PACE">Allure cible (en sec/km ou mile)</string>
  <string id="PARAM_DESC_TARGET_PACE">Exemple:330 pour 5min et 30sec (km/mile choisi
    automatiquement selon les paramètres de la montre)</string>
  <string id="PARAM_PACE_RANGE">Plage d'allure (+/- en sec)</string>
  <string id="PARAM_DESC_PACE_RANGE">Utiliser pour afficher la vitesse/allure en couleur si en
    dehors de la plage</string>

  <string id="PARAM_LAP_DISTANCE">Distance du tour (en mètres)</string>
  <string id="PARAM_DESC_LAP_DISTANCE">Correction automatique de la distance lorsque le bouton de
    tour est pressé (0=désactivé,
    1000=1km, 1609=1mile)</string>

  <string id="PARAM_HEADER_HEIGHT">Hauteur du titre (%)</string>
  <string id="PARAM_DESC_HEADER_HEIGHT">0 pour cacher les titres</string>

  <string id="PARAM_ROW_HEIGHT_RATIO">Ratio de hauteur de ligne</string>
  <string id="PARAM_DESC_ROW_HEIGHT_RATIO">Exemple:4,7,7,3,3 signifie que la ligne #1 occupe 4/24 de
    l'écran,
    lignes #2 et #3 occupent 7/24, etc.</string>

  <string id="PARAM_COLUMN_WIDTH_RATIO_1">Ratio de largeur de colonne pour la deuxième ligne</string>
  <string id="PARAM_DESC_COLUMN_WIDTH_RATIO_1">Exemple:1,2,1 signifie que la colonne du milieu sera
    2 fois
    plus grande que les colonnes 1 et 3</string>
  <string id="PARAM_COLUMN_WIDTH_RATIO_2">Ratio de largeur de colonne pour la troisième ligne</string>

